alt - j : yabai -m window --focus south
alt - k : yabai -m window --focus north
alt - h : yabai -m window --focus west
alt - l : yabai -m window --focus east

cmd + alt - r : yabai -m space --rotate 270
cmd + alt - y : yabai -m space --mirror y-axis
cmd + alt - x : yabai -m space --mirror x-axis
cmd + alt - t : yabai -m window --toggle float --grid 4:4:1:1:2:2

cmd + alt - f : yabai -m window --toggle zoom-fullscreen
cmd + alt - e : yabai -m space --balance

cmd + alt - j : yabai -m window --swap south
cmd + alt - k : yabai -m window --swap north
cmd + alt - h : yabai -m window --swap west
cmd + alt - l : yabai -m window --swap east

shift + alt - h : yabai -m window --display west; yabai -m display --focus west;
shift + alt - l : yabai -m window --display east; yabai -m display --focus east;

shift + alt - p : yabai -m window --space prev
shift + alt - n : yabai -m window --space next

shift + alt - 1 : yabai -m window --space 1
shift + alt - 2 : yabai -m window --space 2
shift + alt - 3 : yabai -m window --space 3
shift + alt - 4 : yabai -m window --space 4
shift + alt - 5 : yabai -m window --space 5

ctrl + alt - q : yabai --stop-service
ctrl + alt - s : yabai --start-service

cmd - i : yabai -m config top_padding 24; yabai -m config bottom_padding 0; yabai -m config left_padding 0 yabai -m config right_padding 0; yabai -m config window_gap 0;
cmd + alt - i: yabai -m config top_padding 36; yabai -m config bottom_padding 12; yabai -m config left_padding 12; yabai -m config right_padding 12; yabai -m config window_gap 12;
